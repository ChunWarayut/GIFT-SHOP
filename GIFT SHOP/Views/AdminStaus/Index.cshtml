@model IEnumerable<GIFT_SHOP.Models.Stau>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>ระบบจัดการสถานะการชำระเงิน</h2>

<p>
    @Html.ActionLink("เพิ่มข้อมูล", "Create")
</p>
<table class="table table-hover table-bordered">
    <tr class="thead-light">
        <th>
            @Html.DisplayNameFor(model => model.Staus_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Staus_name)
        </td>
        <td>
            <center>
                @Html.ActionLink("แก้ไข", "Edit", new { id = item.Staus_ID }) |
                @Html.ActionLink("ลบ", "Delete", new { id = item.Staus_ID })
            </center>
        </td>
    </tr>
}

</table>
